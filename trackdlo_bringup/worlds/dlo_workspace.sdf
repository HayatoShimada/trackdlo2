<?xml version="1.0" ?>
<sdf version="1.8">
  <world name="dlo_workspace">

    <!-- ============================================================ -->
    <!-- Physics: 1 ms step, real-time factor 1.0                     -->
    <!-- ============================================================ -->
    <physics name="1ms" type="ignored">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
    </physics>

    <!-- ============================================================ -->
    <!-- Plugins                                                      -->
    <!-- ============================================================ -->
    <plugin filename="ignition-gazebo-physics-system"
            name="ignition::gazebo::systems::Physics"/>
    <plugin filename="ignition-gazebo-user-commands-system"
            name="ignition::gazebo::systems::UserCommands"/>
    <plugin filename="ignition-gazebo-scene-broadcaster-system"
            name="ignition::gazebo::systems::SceneBroadcaster"/>
    <plugin filename="ignition-gazebo-sensors-system"
            name="ignition::gazebo::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>
    <plugin filename="ignition-gazebo-contact-system"
            name="ignition::gazebo::systems::Contact"/>

    <!-- ============================================================ -->
    <!-- Lighting                                                     -->
    <!-- ============================================================ -->

    <!-- Directional sun -->
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>

    <!-- Point fill light -->
    <light type="point" name="fill_light">
      <pose>1 1 3 0 0 0</pose>
      <diffuse>0.5 0.5 0.5 1</diffuse>
      <specular>0.1 0.1 0.1 1</specular>
      <attenuation>
        <range>20</range>
        <constant>0.5</constant>
        <linear>0.01</linear>
        <quadratic>0.01</quadratic>
      </attenuation>
    </light>

    <!-- Point back fill light (reduce shadows) -->
    <light type="point" name="back_fill_light">
      <pose>-0.5 -0.5 2.5 0 0 0</pose>
      <diffuse>0.4 0.4 0.4 1</diffuse>
      <specular>0.05 0.05 0.05 1</specular>
      <attenuation>
        <range>20</range>
        <constant>0.5</constant>
        <linear>0.01</linear>
        <quadratic>0.01</quadratic>
      </attenuation>
    </light>

    <!-- ============================================================ -->
    <!-- Ground Plane: 100x100, light gray                            -->
    <!-- ============================================================ -->
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- ============================================================ -->
    <!-- Table: static, white                                         -->
    <!-- ============================================================ -->
    <model name="table">
      <static>true</static>
      <pose>0.5 0 0.375 0 0 0</pose>
      <link name="table_link">
        <collision name="collision">
          <geometry>
            <box><size>1.2 0.8 0.75</size></box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box><size>1.2 0.8 0.75</size></box>
          </geometry>
          <material>
            <ambient>1.0 1.0 1.0 1</ambient>
            <diffuse>1.0 1.0 1.0 1</diffuse>
            <specular>0.3 0.3 0.3 1</specular>
          </material>
        </visual>
      </link>
    </model>

    <!-- ============================================================ -->
    <!-- RealSense D435: static RGBD camera, mounted above workspace -->
    <!--   Color: 1280x720 @30fps, HFOV 69.4deg (1.2112 rad)        -->
    <!--   Depth: 1280x720 @30fps, HFOV 87deg (1.5184 rad)          -->
    <!--   Range: 0.105m - 10m, Baseline: 50mm                      -->
    <!-- ============================================================ -->
    <model name="realsense_d435">
      <static>true</static>
      <pose>0.5309 0.0301 1.250 0 1.5708 0</pose>
      <link name="camera_link">
        <!-- D435 body: 90mm x 25mm x 25mm -->
        <visual name="d435_body">
          <geometry>
            <box><size>0.025 0.09 0.025</size></box>
          </geometry>
          <material>
            <ambient>0.6 0.6 0.6 1</ambient>
            <diffuse>0.75 0.75 0.75 1</diffuse>
          </material>
        </visual>
        <!-- Front face (black) - at +X (viewing direction) -->
        <visual name="d435_front">
          <pose>0.0135 0 0 0 0 0</pose>
          <geometry>
            <box><size>0.002 0.09 0.025</size></box>
          </geometry>
          <material>
            <ambient>0.05 0.05 0.05 1</ambient>
            <diffuse>0.1 0.1 0.1 1</diffuse>
          </material>
        </visual>
        <!-- Left IR lens indicator -->
        <visual name="d435_ir_left">
          <pose>0.014 0.02 0 0 0 0</pose>
          <geometry>
            <cylinder><radius>0.004</radius><length>0.002</length></cylinder>
          </geometry>
          <material>
            <ambient>0.02 0.02 0.02 1</ambient>
            <diffuse>0.05 0.05 0.05 1</diffuse>
          </material>
        </visual>
        <!-- Right IR lens indicator -->
        <visual name="d435_ir_right">
          <pose>0.014 -0.03 0 0 0 0</pose>
          <geometry>
            <cylinder><radius>0.004</radius><length>0.002</length></cylinder>
          </geometry>
          <material>
            <ambient>0.02 0.02 0.02 1</ambient>
            <diffuse>0.05 0.05 0.05 1</diffuse>
          </material>
        </visual>
        <!-- Color lens indicator -->
        <visual name="d435_color_lens">
          <pose>0.014 0.0348 0 0 0 0</pose>
          <geometry>
            <cylinder><radius>0.003</radius><length>0.002</length></cylinder>
          </geometry>
          <material>
            <ambient>0.0 0.0 0.1 1</ambient>
            <diffuse>0.0 0.0 0.2 1</diffuse>
          </material>
        </visual>
        <!-- Mount bracket (at -X, extends upward via rotation) -->
        <visual name="d435_mount">
          <pose>-0.06 0 0 0 -1.5708 0</pose>
          <geometry>
            <cylinder><radius>0.012</radius><length>0.08</length></cylinder>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.3 0.3 0.3 1</diffuse>
          </material>
        </visual>

        <!-- RGBD sensor matching D435 color camera specs -->
        <sensor name="d435_rgbd" type="rgbd_camera">
          <always_on>1</always_on>
          <update_rate>30</update_rate>
          <camera>
            <!-- D435 color HFOV: 69.4 deg = 1.2112 rad -->
            <horizontal_fov>1.2112</horizontal_fov>
            <image>
              <width>1280</width>
              <height>720</height>
              <format>R8G8B8</format>
            </image>
            <clip>
              <near>0.105</near>
              <far>10.0</far>
            </clip>
            <depth_camera>
              <clip>
                <near>0.105</near>
                <far>10.0</far>
              </clip>
            </depth_camera>
            <!-- Gaussian noise to simulate real D435 depth noise -->
            <noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.002</stddev>
            </noise>
          </camera>
          <topic>camera</topic>
        </sensor>
      </link>
    </model>

